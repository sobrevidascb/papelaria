CREATE TABLE acess(
	id SERIAL NOT NULL,
	pw varchar(30),
	constraint pk_psw PRIMARY KEY (id)
);

CREATE TABLE genero(
	id SERIAL NOT NULL,
	nome varchar(30) NOT NULL,
	CONSTRAINT pk_genero PRIMARY KEY(id)
);

